% M
igm = 1.3; % mS/cm^2 // 1.2 parkinsonian, 1.3 normal
iEk = -100;
Q10 = 3.209;
vhalf = -30;
IC_noise=0;
w' = (Q10*0.0001*(X-vhalf)./(1-exp(-(X-vhalf)/9))).*(1-w) + Q10*0.0001*(X-vhalf)./(1-exp((X-vhalf)/9)).*w
w(0)=.0249244+IC_noise.*rand(1,Npop)
iM(X,w)=igm.*w.*(X-iEk)
@current += -iM(X,w)

